<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/titleTablayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/px48"
        android:layout_marginStart="@dimen/px16"
        android:ellipsize="end"
        android:lines="1"
        android:text="平均通关率"
        android:textColor="@color/black"
        android:textSize="@dimen/SP14"
        android:gravity="center_vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="439"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/countTablayout"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/countTablayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:minHeight="@dimen/px48"
        android:gravity="center"
        android:text="65%"
        android:textColor="@color/black"
        android:textSize="@dimen/SP14"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_weight="217"
        app:layout_constraintLeft_toRightOf="@+id/titleTablayout"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:layout_width="@dimen/px1"
        android:layout_height="0dp"
        app:layout_constraintVertical_weight="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@color/color_b2"
        app:layout_constraintLeft_toRightOf="@+id/titleTablayout"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px1"
        app:layout_constraintTop_toBottomOf="@+id/countTablayout"
        android:background="@color/color_b2"/>
</androidx.constraintlayout.widget.ConstraintLayout>